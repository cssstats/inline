{
  "name": "inline",
  "description": "Get inline css api",
  "author": "John Otander",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "preinstall": "apt-get update && apt-get install -y libgtk2.0-0 libgconf-2-4 libasound2 libxtst6 libxss1 libnss3 xvfb",
    "dev": "micro -p 3000 index.js",
    "start": "xvfb-run --server-args=\"-screen 0 1280x1028x24 -ac +extension GLX +render\" micro -p 3000 index.js"
  },
  "repository": "cssstats/inline",
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "cssstats": "^3.0.0-beta.2",
    "get-inline": "^0.1.2",
    "get-query-param": "^1.0.0",
    "micro": "^6.1.0",
    "normalize-url": "^1.7.0"
  },
  "devDependencies": {
    "ava": "*"
  }
}
